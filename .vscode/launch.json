{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Pinata Upload Files",
      "program": "${workspaceFolder}/src/upload-files.js",
      "skipFiles": ["<node_internals>/**"],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Pinata Download CIDs",
      "program": "${workspaceFolder}/src/download-cids.js",
      "skipFiles": ["<node_internals>/**"],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Calculate File CIDs",
      "program": "${workspaceFolder}/src/calculate-cids.js",
      "skipFiles": ["<node_internals>/**"],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
